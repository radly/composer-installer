{
    "name": "radphp/composer-installer",
    "type": "composer-installer",
    "license": "MIT",
    "description": "Installs RadPHP bundles to the \"bundles\" directory.",
    "keywords": ["RadPHP", "installer", "composer"],
    "authors": [
        {
            "name": "Mohammad Abdoli Rad",
            "email": "m.abdolirad@gmail.com",
            "role": "Project Founder"
        },
        {
            "name": "Omid Mottaghi Rad",
            "email": "omidmr@gmail.com",
            "role": "Project Founder"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "composer/composer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Rad\\Composer\\": "src"
        }
    },
    "extra" : {
        "class" : "Rad\\Composer\\BundleInstaller"
    }
}
